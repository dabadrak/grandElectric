                    tr
                      each val in ['Подрозетник кирпич','290',"шт"]
                        td= val
                    tr
                      each val in ['Подрозетник бетон','390',"шт"]
                        td= val
                    tr
                      each val in ['Сквозное сверление стен','140',"шт"]
                        td= val
                h3 Щитовое оборудование
                .miniPrice
                  table
                    tr
                      each val in ['Счётчик однофазный','790', 'шт']
                        td= val
                    tr
                      each val in ['Счётчик трёхфазный','1990',"шт"]
                        td= val
                    tr
                      each val in ['Монтаж щитка','690',"шт"]
                        td= val
                    tr
