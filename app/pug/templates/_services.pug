section.servicesWrap
  #servicesTabs
    .servicesPriceNavBg
      .content
        ul.servicesPriceNav
          li.active
            figure
              .imgContainer.imgContainer__1
                img(src='img/servicesComplex.png')
              figcaption Проводка<br>в&nbsp;квартире<br>и&nbsp;в&nbsp;доме
            a(href='#servicesTabs1').labelScreen
          li
            figure
              .imgContainer.imgContainer__2
                img(src='img/servicesTools.png')
              figcaption Установка<br>электро-<br>приборов
            a(href='#servicesTabs2').labelScreen
          li
            figure
              .imgContainer.imgContainer__3
                img(src='img/servicesCommercial.png')
              figcaption Коммер-<br>ческая<br>электрика
            a(href='#servicesTabs3').labelScreen
    .servicesTabs
      article.servicesTab#servicesTabs1
        .content
          h2 Устройство электропроводки
          .servicesTabContentWrap
            .servicesTabContent
              #accordionWiring.accordion
                h3 Проводка
                .miniPrice
                  table
                    tr
                      each val in ['Демонтаж кабеля','30','м']
                        td= val
                    tr
                      each val in ['Прокладка кабеля','50',"м"]
                        td= val
                    tr
                      each val in ['Затягивание кабеля в гофру','39',"м"]
                        td= val
                    tr
                      each val in ['Монтаж подвесного лотка','300',"м"]
                        td= val
                    tr
                      each val in ['Монтаж кабель-канала','95',"м"]
                        td= val
                    tr
                      each val in ['Распаячня коробка','490',"шт"]
                        td= val
                    tr
                      each val in ['Распаячня коробка сварка','890',"шт"]
                        td= val
                h3 Штробление
                .miniPrice
                  table
                    tr
                      each val in ['Штробление стен кирпич','290', 'м']
                        td= val
                    tr
                      each val in ['Штробление стен бетон','340',"м"]
                        td= val
                    tr
                      each val in ['Ниша под эл. щиток кирпич','1990',"шт"]
                        td= val
                    tr
                      each val in ['Ниша под эл. щиток бетон','4490',"шт"]
